config webfilter profile
    edit "g-default"
        set comment "Default web filtering."
        unset options
        config file-filter
            set status enable
            set log enable
            set scan-archive-contents enable
        end
        set https-replacemsg enable
        set post-action normal
        config web
            set blacklist disable
        end
        set youtube-channel-status disable
        config ftgd-wf
            unset options
            set exempt-quota 17
            config filters
                edit 1
                    set category 0
                    set action block
                    set log enable
                next
                edit 2
                    set category 2
                    set action block
                    set log enable
